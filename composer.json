    "symfony/yaml": "^7",
    "symfony/cache": "^7",
    "webonyx/graphql-php": "^15.19.1",
    "laminas/laminas-servicemanager": "^4.4",
    "nikic/fast-route": "^1.3",
    "wappcode/pdss-utilities": "dev-V4.0.0-SNAPSHOOT"
