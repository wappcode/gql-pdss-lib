{
  "version": "4.1.1",
  "name": "wappcode/gqlpdss",
  "description": "Utilidades para crear una api GraphQL",
  "type": "library",
  "require": {
    "php": ">=8.2",
    "doctrine/orm": "^3",
    "doctrine/dbal": "^4",
    "symfony/yaml": "^7",
    "symfony/cache": "^7",
    "webonyx/graphql-php": "^15.19.1",
    "laminas/laminas-servicemanager": "^4.4",
    "nikic/fast-route": "^1.3",
    "wappcode/pdss-utilities": "dev-master",
    "symfony/var-exporter": "^7.0",
    "psr/http-message": "^2.0",
    "psr/http-server-middleware": "^1.0",
    "laminas/laminas-diactoros": "^3.8",
    "psr/http-factory": "^1.1",
    "laminas/laminas-httphandlerrunner": "^2.13"
  },
  "autoload": {
    "files": [
      "GPDCore/src/Functions/PaginationUtilities.php",
      "GPDCore/src/Functions/StringFunctions.php"
    ],
    "psr-4": {
      "GPDCore\\": "GPDCore/src/",
      "GraphqlModule\\": "GraphqlModule/src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "AppModule\\": "dev/modules/AppModule/src/"
    }
  },
  "require-dev": {
    "phpunit/phpunit": "^9.5",
    "wappcode/graphql-basic-client": "^1",
    "friendsofphp/php-cs-fixer": "^3.0",
    "phpoffice/phpspreadsheet": "^5.4"
  },
  "license": "MIT",
  "scripts": {
    "cs-fix": "php-cs-fixer fix",
    "cs-fix-dry": "php-cs-fixer fix --dry-run --diff",
    "cs-check": "php-cs-fixer fix --dry-run --diff --verbose",
    "test": "phpunit --colors=always",
    "doctrine": "bin/doctrine"
  }
}